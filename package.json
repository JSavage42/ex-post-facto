{
  "name": "ex-post-facto",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "homepage": "http://expostfacto.dev",
  "description": "Agile Retro Board Tool",
  "repository": {
    "type": "git",
    "url": "https://github.com/jsavage42/ex-post-facto.git"
  },
  "keywords": [
    "agile",
    "retro",
    "scrum",
    "tools",
    "teams"
  ],
  "author": "Jason A. Savage",
  "scripts": {
    "start": "concurrently 'npm run server-start' 'npm run client-start'",
    "server": "DEBUG=app:* nodemon ./src/server/server.js",
    "start-dev": "concurrently 'npm run server-start-dev' 'npm run client-start-dev'",
    "build": "npm run server-build && npm run client-build",
    "server-build": "cd src && cd server && npm i --silent",
    "client-build": "react-scripts build",
    "server-start": "cd src && cd server && npm run start",
    "client-start": "react-scripts start",
    "server-start-dev": "npm run server",
    "client-start-dev": "npm run client-start",
    "lint:js": "eslint .",
    "lint:css": "stylelint .",
    "test": "npm run lint:js && npm run lint:css",
    "eject": "react-scripts eject"
  },
  "bugs": {
    "url": "https://github.com/jsavage42/ex-post-facto/issues"
  },
  "homepage": "https://github.com/jsavage42/ex-post-facto#readme",
  "dependencies": {
    "axios": "^0.19.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http2": "^3.3.7",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.8.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "sse-express": "^1.0.2",
    "styled-components": "^4.4.1",
    "validator": "^12.1.0",
    "xlsx": "^0.15.4"
  },
  "devDependencies": {
    "debug": "^4.1.1",
    "concurrently": "^5.0.2",
    "eslint": "^6.7.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "nodemon": "^2.0.2",
    "stylelint": "^13.0.0",
    "stylelint-config-standard": "^19.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
